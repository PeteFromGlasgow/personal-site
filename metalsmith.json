{
  "metadata": {
    "title": "My Blog",
    "description": "My second, super-cool blog."
  },
  "plugins": {
    "metalsmith-markdown": {},
    "metalsmith-permalinks": {
      "pattern": ":title"
    },
    "metalsmith-layouts": {
      "engine": "swig"
    },
    "metalsmith-assets": {
      "source": "./public",
      "destination": "./public"
    },
    "metalsmith-concat": {
      "files": [
        "public/components/jquery/dist/jquery.min.js",
        "public/components/foundation/dist/foundation.min.js",
        "public/components/what-input/what-input.min.js"
      ],
      "output": "public/vendor.js"
    },
    "metalsmith-sass": {
      "outputStyle": "expanded"
    }
  }
}
